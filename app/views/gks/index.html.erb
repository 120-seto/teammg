<%= render "shared/header" %>
   <div class="gk-new">
  <% if user_signed_in? %>
   <%= link_to "GK練習投稿", new_gk_path, class: "gk" %>
  <% end %>
  <div class="contents row">
    <% @gks.each do |gk| %>
        <p><%= gk.text %></p>
        <span class="name">
          <%= gk.name %>
        </span>
    <% end %>
         <%= render "shared/trs" %>
    </div>
  </div>
  <%#= image_tag gk.image.variant(resize: '500x500'), class: 'gk-image' if gk.image.attached? %>
 </div>